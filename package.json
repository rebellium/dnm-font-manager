{
  "name": "dnm-font-manager",
  "version": "0.5.2",
  "description": "Search system fonts with family and style using pure JavaScript, forked from rBurgett/system-font-families",
  "main": "dist/main.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "babel-plugin-transform-runtime": "^6.22.0",
    "watch": "babel src --watch --out-dir dist",
    "test": "mocha --compilers js:babel-register",
    "git": "npm run build && git add . && git commit -m",
    "postgit": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdanim/dnm-font-manager.git"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.7.2",
    "eslint": "^3.14.1",
    "fontkit": "^1.8.1",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "should": "^8.3.0"
  },
  "author": "Adrien du Repaire",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/devdanim/dnm-font-manager/issues"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "file-type": "^10.11.0",
    "read-chunk": "^3.2.0",
    "ttfinfo": "git+https://github.com/rBurgett/ttfinfo.git"
  }
}
